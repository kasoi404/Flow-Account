{
  "name": "flowlite",
  "version": "1.0.0",
  "description": "A minimal, production-ready Next.js application with Prisma",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "pack:zip": "mkdir -p dist && zip -r dist/flowlite-source.zip . -x \"node_modules/*\" \".next/*\" \"dist/*\" \".git/*\" \"*.log\" \"*.zip\""
  },
  "dependencies": {
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@prisma/client": "^5.16.2",
    "clsx": "^2.1.1",
    "tailwindcss": "^3.4.4"
  },
  "devDependencies": {
    "prisma": "^5.16.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.39"
  }
}